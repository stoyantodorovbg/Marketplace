<h1>Login</h1>

<form method="post" action="{{ path('security_login') }}" >
<div>
    <label>Email:</label>
    <input type="text">
</div>
    <div>
        <label>Password:</label>
        <input type="password">
    </div>
    <div>
        <input type="hidden" name="_csrf_token" value="{{ csrf_token('authenticate') }}"/>
        <input type="submit">
    </div>
</form>